<div
  class="row rounded shadow justify-content-center bgwt p-2 mb-3 text-center"
>
  <h1 class="slab roxo">Busca por usu√°rios</h1>
</div>
<div class="row row-cols-1 row-cols-md-5 g-3">
  <% usuarios.forEach(usuario=> { %> <% if (usuario.id!=locals.layoutVariables.usuario.id || locals.layoutVariables.usuario.id == false){ %>
  <div class="col rounded animalcard">
    <div class="card h-100 shadow-sm">
      <img
        src="<%- usuario.foto %>"
        class="card-img-top img-fluid img-cards"
        alt=""
      />
      <div class="card-body rounded-bottom">
        <h5 class="card-title roxo slab"><%- usuario.nome %></h5>
        <a href="/users/<%- usuario.id %>" class="btn btn-light lara mt-2">Ver mais</a>
      </div>
    </div>
  </div>
  <%}});%>
</div>
